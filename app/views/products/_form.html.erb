<%= form_for [@warehouse, @product] do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :price %><br>
  <%= f.number_field :price, min: 1 %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>

  <%= f.label :quantity %><br>
  <%= f.number_field :quantity, min: 1 %><br>

  <h3>Add to User</h3>
  <%= f.collection_select :user_id, User.all, :id, :name %><br>

  <h3>Which Warehouses: </h3>
  <table>
    <tr class="header_container">
      <th class="header"></th>
      <th class="header">Name</th>
    </tr>
    <%= f.collection_check_boxes :warehouse_ids, Warehouse.all, :id, :name do |b| %>
      <tr class="data_container">
        <td class="data"> <%= b.check_box %> </td>
        <td> <%= b.text %> </td>
      </tr>
    <% end %><br>
  </table>
  <br><%= f.submit %>
<% end %><br>